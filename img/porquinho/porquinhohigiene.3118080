<script>
	function updateWidgets() {
		const hidden = 'hidden';
		const activeClass = 'color-highlight';
		const articles = document.querySelectorAll('article');

		articles.forEach((article) => {
		const nav = article.querySelectorAll('.widgetGroupMenus button');
		nav.forEach((item) => {
			item.addEventListener('click', ({ target }) => {
				event.preventDefault();
				const widgetClass = `${item.getAttribute('data-widget')}`;

				nav.forEach(item => item.classList.remove(activeClass));
				target.classList.add(activeClass);

				const widgets = article.querySelectorAll('.widget');
				widgets.forEach(widget => {
					if (!widget.classList.contains(widgetClass)) {
						widget.classList.add(hidden)
					} else {
						widget.classList.remove(hidden);
					}
				});
			});
		});
	});
	}
	updateWidgets();
</script>



